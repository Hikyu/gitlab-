{
  "title": "Gitlab 帮助手册",
  "description": "本书用于指导神通内部 Gitlab 的使用。",
  "language": "zh",
  "plugins": [
    "advanced-emoji",
    "alerts",
    "anchor-navigation-ex",
    "anchors"
  ],
  "pluginsConfig": {
    "anchor-navigation-ex": {
      "showLevel": true,
      "associatedWithSummary": true,
      "printLog": false,
      "multipleH1": true,
      "mode": "float",
      "float": {
        "showLevelIcon": false,
        "level1Icon": "fa fa-hand-o-right",
        "level2Icon": "fa fa-hand-o-right",
        "level3Icon": "fa fa-hand-o-right"
      },
      "pageTop": {
        "showLevelIcon": false,
        "level1Icon": "fa fa-hand-o-right",
        "level2Icon": "fa fa-hand-o-right",
        "level3Icon": "fa fa-hand-o-right"
      }
    }
}
}